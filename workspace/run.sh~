cd /home/hc475/compiler/collectInfo
ninja

VAR=$?
cd /home/hc475/compiler/workspace
cd src
dir=/home/hc475/compiler/workspace
declare -a arr=("tmp" )


if [ $VAR -eq 0 ]; then 
	echo "start"
	echo "start" > $dir/info.txt
	for i in "${arr[@]}" 
	do
		echo "in" $i
		echo "--------------------" $i "--------------------" >> $dir/info.txt
		cd $i
		make CFLAGS='' CXXFLAGS='-Xclang -load -Xclang /home/hc475/compiler/collectInfo/info.so'
		cd ..
		echo "----------------------------------------------" >> $dir/info.txt
		echo $'\n' >> $dir/info.txt
		
		
	done
fi

